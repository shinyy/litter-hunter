@import '../../app.wxss';
@import '../../pages/home/address/address.wxss';
.addressBlock{
  position:fixed;
  width:100%;
  height:calc(100vh - 150rpx);
  z-index:99999;
  top:0;
  background:#f5f5f5;

}
.addressBlock.sp{
  top: 80rpx;
  height: calc(100vh - 80rpx);
}
.scrollbody{
  height: 100%;
  position: absolute;
  box-sizing: border-box;
}
.slideup{
  color: #f31d53;
  text-align: center;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
}