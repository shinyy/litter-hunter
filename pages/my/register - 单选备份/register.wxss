/* pages/my/register/register.wxss */
page{
  padding-top: 20rpx;
}
.ul{
  padding: 0 30rpx;
  font-size: 30rpx;
}
.li{
  height: 100rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.til{
  width: 160rpx;
}
.getCode{
  width: 200rpx;
  height: 80rpx;
  font-size: 28rpx;
  background: #ccc;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
}
.submit-btn{
  width: 100%;
  height: 100rpx;
  background: #f31d53;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  line-height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
}
/* 行业弹出页 */
.slideup{
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  color: #f31d53;
  font-size: 28rpx;
  background: #f5f5f5;
}
.left-part{
  position: absolute;
  top: 60rpx;
  left: 0;
  width: 330rpx;
  height: calc(100vh - 60rpx);
  z-index: 999;
  background: #f5f5f5;
}
.left-part>view,.left-part .list{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  padding-left: 47rpx;
  box-sizing: border-box;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.left-part>.cur,.left-part .list.cur{
  background: #fff;
}
.right-part{
  position: absolute;
  top: 60rpx;
  left: 0;
  padding-left: 390rpx;
  width: 100%;
  height: calc(100vh - 60rpx);
  box-sizing: border-box;
}
.right-part>view,.right-part .list{
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx;
}
.right-part>.cur,.right-part .list.cur{
  color: #f31d53;
}
.industryBlock{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 100;
}

/* 地图弹出页 */
.mapBlock{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 100;
}

